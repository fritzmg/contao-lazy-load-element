{
	"name":"fritzmg/contao-lazy-load-element",
	"description":"Contao extension to allow lazy loading of content elements or modules.",
	"keywords":["contao", "lazy", "load", "content", "module", "element"],
	"type":"contao-bundle",
	"homepage":"http://www.inspiredminds.at",
	"license":"LGPL-3.0-or-later",
	"authors":[
		{
			"name":"Fritz Michael Gschwantner",
			"homepage":"http://www.inspiredminds.at",
			"email":"fmg@inspiredminds.at",
			"role":"Developer"
		}
	],
	"support":{
		"email":"fmg@inspiredminds.at",
		"issues":"https://github.com/fritzmg/contao-lazy-load-element/issues",
		"source":"https://github.com/fritzmg/contao-lazy-load-element",
		"forum":"https://community.contao.org/de"
	},
	"funding": [
        	{
			"type": "github",
			"url": "https://github.com/sponsors/fritzmg"
		}
	],
	"require":{
		"php": ">=7.4",
		"contao/core-bundle": "^4.13"
	},
	"autoload": {
		"psr-4": {
			"InspiredMinds\\ContaoLazyLoadElement\\": "src"
		},
		"classmap": [
			"contao/classes"
		]
	},
	"extra":{
		"contao-manager-plugin": "InspiredMinds\\ContaoLazyLoadElement\\ContaoManager\\Plugin"
	}
}
